<section class="fuel-choose-your-membership">
  <div class="fuel-choose-container">
    <div class="choose-first-section">
      <h1 class="choose-title">Choose Your Membership</h1>
      <h1 class="choose-title mobile-version" style="display:none;">Choose Membership</h1>
      <div class="icon-choose-one">
        {%comment%}
          <div class="icon-svg-fuel">{% render 'award'%}</div>
          <h3>90-day Guarantee</h3>
        {%endcomment%}
      </div>
      <div class="icon-choose-one"></div>
    </div>

    {%comment%}
      {% for product in collections['all'].products %}
      {% if product.handle == 'pre-order-bloom' %}
      {% for variant in product.variants %}
      {% if variant.title contains '4 cards per month / (6-month Membership)' %}
      <div class="price-flex-container-flex">
      <span class="money-span">$</span>
      {{variant.price | divided_by: 100.00 | divided_by: 1 | json}}
      <span class="month-text">/ month</span>
      </div>
      {% if section.settings.discount_ribbon == blank %}
      <span class="price-discount-ribbon"></span>

      {%else%}
      <span class="price-discount-ribbon">{{section.settings.discount_ribbon}}</span>
      {%endif%}
      {% for allocation in variant.selling_plan_allocations %}
      {% assign selling_id = allocation.selling_plan.id  %}
      {% endfor %}
      {%endif%}
      {% endfor %}
      {%endif%}
      {%endfor%}
      <button class="add-to-cart-flex-page button flex-page-btn" data-cart="{{ section.settings.variant_id }}" data-selling="{{selling_id}}">{{ section.settings.title_buy_now }}
      <div class="loader-me"></div>
      </button>

    {%endcomment%}

    <div class="choose-second-section">
      <div class="price-container">
        <span class="popular-tag-fuel">Best Value</span>
        <div class="price-section-one">
          <h3 class="price-title">12 Month</h3>
          <p>Membership</p>
        </div>
        <div class="price-tag">
          <span class="dolar-tag">$</span>
          <div class="fuel-price">35
            <span>/ month</span>
          </div>
          <span class="price-day">4 cards per month</span>
          <span class="price-day mobile-day" style="color:red;" style="visibility:hidden;">Save $180</span>
          <span class="save-day" style="visibility:hidden;">Save $120</span>
        </div>
        <div class="commitment-price">
          <p>Free Shipping</p>
          <button class="cta-fuel-btn" data-cart="40619087724730" data-selling="718405818">Choose</button>
        </div>
      </div>
      <div class="price-container">
        <span class="popular-tag-fuel">Most Popular</span>
        <div class="price-section-one">
          <h3 class="price-title">6 Month</h3>
          <p>Membership</p>
        </div>
        <div class="price-tag">
          <span class="dolar-tag">$</span>
          <div class="fuel-price">40
            <span>/ month</span>
          </div>
          <span class="price-day">4 cards per month</span>
          <span class="price-day mobile-day" style="color:red;" style="visibility:hidden;">Save $60</span>
          <span class="save-day" style="visibility:hidden;">Save $120</span>
        </div>
        <div class="commitment-price">
          <p>Free Shipping</p>
          <button class="cta-fuel-btn" data-cart="41061295751354" data-selling="718405818">Choose</button>
        </div>
      </div>
      <div class="price-container">
        <div class="price-section-one">
          <h3 class="price-title">Month-to-Month</h3>
          <p>Membership</p>
        </div>
        <div class="price-tag">
          <span class="dolar-tag">$</span>
          <div class="fuel-price">50
            <span>/ month</span>
          </div>
          <span class="price-day last-price-day" style="margin-top:-54px;">4 cards per month</span>
        </div>
        <div class="commitment-price">
          <p>Free Shipping</p>
          <button class="cta-fuel-btn" data-cart="40619087626426" data-selling="718405818">Choose</button>
        </div>
      </div>
    </div>
  </div>
</section>

<script>

  setTimeout(function() {
    jQuery.ajax({type: 'POST', url: '/cart/clear.js', dataType: 'json'});
  }, 400);

  $(".cta-fuel-btn").click(function() {

    let variantId = $(this).data('cart');
    let sellingPlan = $(this).data('selling');
    let qty = 1;
    console.log(variantId);
    console.log(sellingPlan);
    console.log(qty);

    function addItemToCart() {
      data = {
        "id": variantId,
        "quantity": qty,
        "selling_plan": sellingPlan
      }

      jQuery.ajax({
        type: 'POST',
        url: '/cart/add.js',
        data: data,
        dataType: 'json',
        success: function() {
          window.location = '/checkout';
        }
      });

// window.location = '/checkout';
    }addItemToCart();

  });
</script>

{% schema %}
  {
    "name": "fuel-membership",
    "class": "intro-class",

    "settings": [
      {
        "type": "text",
        "id": "variant_id",
        "placeholder": "Monthly",
        "default": "41532525478074",
        "info": "Variant ID 1",
        "label": "Variant ID fuel"
      }, {
        "type": "text",
        "id": "selling_id",
        "placeholder": "3 Month",
        "default": "718405818",
        "info": "Selling ID 2",
        "label": "Selling ID fuel"
      }
    ]
  }
{% endschema %}