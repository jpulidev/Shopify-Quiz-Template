  }
  .image-with-text__content{
    align-items: center;
  }
  .rg-heading-slide {
    position: relative;

  }
  .mobile-heading {
    margin: 0;
    display: grid;
    grid-template-columns: 1fr 1fr;
    padding-left: 7px;
  }
  .video-hp-mobile {
    height: 228px;
    position: absolute;
    right: 0;
    transform: rotate(-18deg);
    top:24px;
    margin-right: 68px;
    width: 105px;
  }
  .desktop-text{
    display:none;
  }
  .mobile-span-text {
    position: absolute;
  }
}

