    left: 11px;
 }  
 .slider-b {
  top:-132px!important;
 }
.slider-c>.slick-list>.slick-track>.slick-active>img {
  height: 360px!important;
    margin-top: 0px!important;
    margin-left: 30px;
    z-index: 1;
    }
.slider-b>button {
  display:none!important;
} 
.first-2650181a-2a46-4dce-a267-aadf902ad317 {
  left: 24px!important;
}

 .hero-slider-text-js.slick-initialized.slick-slider {
  height: 73px;
    max-width: 255px;
