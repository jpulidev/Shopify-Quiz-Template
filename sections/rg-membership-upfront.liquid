{% schema %}
{
"name": "Membership 2",
"class": "new-membership-section upfront",
"settings": [
  {
    "type": "text",
    "id": "title",
    "label": "Title",
    "default": "Choose the membership that works best for you"
  },
  {
    "type": "text",
    "id": "subtitle",
    "label": "Sub title",
    "default": "How often would you like to test?"
  }
],
"blocks": [
  {
    "type": "icon",
    "name": "icon",
    "settings": [
      {
        "type": "image_picker",
        "id": "icon",
        "label": "icon"
      },
      {
        "type": "text",
        "id": "title",
        "label": "Title"
      },
      {
        "type": "text",
        "id": "subtitle",
        "label": "Subtitle"
      }
    ]
  }
],
"presets": [
{
"category": "Memebership",
"name":"Featured membership new"
}
]
}
{% endschema %}
