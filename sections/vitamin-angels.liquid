                    <h3>So far, we have reached</h3>
                    <h1 class="we-reached-counter">0</h1>
                    <div class="html-embed-10 w-embed w-script">
                        <script>
                            function numb() {
                                var number = 21000;
                                var $this = $('.we-reached-counter')
                                $({
                                    countNum: $this.text()
                                }).animate({
                                    countNum: number
                                    },
                                    {
                                    duration: 2000,
                                    easing: 'linear',
                                    step: function() {
                                        $this.text(commaSeparateNumber(Math.floor(this.countNum)));
                                    },
                                    complete: function() {
                                        $this.text(commaSeparateNumber(this.countNum));
                                        //alert('finished');
                                    }
                                    }
                                );
                            }
                            function commaSeparateNumber(val) {
                                while (/(\d+)(\d{3})/.test(val.toString())) {
                                    val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2');
                                }
                                return val;
                            }
                            var started = false;
                            $(window).on('scroll',function(){
                                var scroll = $(window).scrollTop();
                                var elem = $(".we-reached-counter").offset().top;
                                var winHeight = $( window ).height();
                                var pos = elem - (winHeight / 1.25)
                                if (scroll >= pos) {
                                    if(!started) {
                                        numb();
                                    }
                                    started = true;
                                }
                            });
                        </script>

