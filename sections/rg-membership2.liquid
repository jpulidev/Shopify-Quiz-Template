<script>
$(".add-to-cart-free").click(function(){
 
 let variantId = $(this).data('productId'); 
 let sellingPlan = $(this).data('sellingPlan');
 let qty = 1;
 console.log(variantId);
 console.log(sellingPlan);  
 console.log(qty);     

     function addItemToCart() {
     data = {
          "id": variantId,
          "quantity": qty,
          "selling_plan":sellingPlan
        }
      
        jQuery.ajax({
        type: 'POST',
        url: '/cart/clear.js',
        dataType: 'json'
      });  

      setTimeout(function() { 
        jQuery.ajax({
        type: 'POST',
        url: '/cart/add.js',
        data: data,
        dataType: 'json',
        success: function() {
          window.location = '/checkout/?step=contact_information';
        }
      });
    }, 500);
      //window.location = '/checkout';
    }
    addItemToCart();
    
});
</script>

